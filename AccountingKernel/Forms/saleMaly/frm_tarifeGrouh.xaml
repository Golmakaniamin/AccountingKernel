                <Label Content="سمت امضا کننده" Style="{StaticResource LabelStyle}" HorizontalAlignment="Left" Margin="618,130,0,0" VerticalAlignment="Top" Width="90"/>
                <TextBox x:Name="txtemza1_semat" Style="{StaticResource TextBoxStyle}" HorizontalAlignment="Left" Margin="713,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" PreviewTextInput="txtemza1_semat_PreviewTextInput"/>
