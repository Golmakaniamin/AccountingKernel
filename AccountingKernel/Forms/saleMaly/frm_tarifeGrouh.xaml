                <TextBox x:Name="txtnoefareiepardakhtkonande" Style="{StaticResource TextBoxStyle}" HorizontalAlignment="Left" Margin="213,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
