                <Label Content="نحوه پرداخت" Style="{StaticResource LabelStyle}" HorizontalAlignment="Left" Margin="368,210,0,0" VerticalAlignment="Top" Width="90"/>
                <ComboBox x:Name="cmbPaymentType" HorizontalAlignment="Left" Height="35" Margin="463,210,0,0" VerticalAlignment="Top" Width="150" DisplayMemberPath="SDesc" SelectedValuePath="Id"/>
