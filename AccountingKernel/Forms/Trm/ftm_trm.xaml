<Window x:Class="AccountingKernel.Forms.Trm.ftm_trm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ftm_trm" Height="600" Width="800" FlowDirection="RightToLeft" Style="{DynamicResource WindowStyle}" WindowStartupLocation="CenterScreen">
    <Grid>
        <DataGrid Margin="10" FlowDirection="RightToLeft" Style="{StaticResource DataGridStyle}" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <!-- <DataGridTextColumn Binding="{Binding Path=Id}" Header="کد" IsReadOnly="False"/> -->
                <DataGridTextColumn  Header="عنوان" IsReadOnly="False"/>
                <DataGridTextColumn  Header="کد حسابداری بدهکار" IsReadOnly="False"/>
                <DataGridTextColumn  Header="کد حسابداری بستانکار" IsReadOnly="False"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
