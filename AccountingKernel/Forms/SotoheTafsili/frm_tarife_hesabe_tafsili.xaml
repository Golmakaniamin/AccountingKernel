        <Label Content=" عنوان لاتین گروه" Style="{StaticResource LabelStyle}" HorizontalAlignment="Left" Margin="326,143,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.037,-0.167" Width="95"/>
