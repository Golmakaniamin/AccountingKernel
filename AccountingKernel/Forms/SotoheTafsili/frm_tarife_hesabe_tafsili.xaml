        <WPFAutoCompleteTextbox:AutoCompleteTextBox x:Name="txt_grouh_eng_name"  HorizontalAlignment="Left" Height="35" Margin="426,143,0,0" VerticalAlignment="Top" Width="140" PreviewTextInput="txt_grouh_eng_name_PreviewTextInput" />
        <Label Content="نام تفصیل" Style="{StaticResource LabelStyle}" HorizontalAlignment="Left" Margin="326,183,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.037,-0.167" Width="95"/>
        <WPFAutoCompleteTextbox:AutoCompleteTextBox x:Name="txt_onvane_hesab1" HorizontalAlignment="Left" Height="35" Margin="426,183,0,0" VerticalAlignment="Top" Width="140" LostFocus="txt_onvane_hesab1_LostFocus" PreviewTextInput="txt_onvane_hesab1_PreviewTextInput"/>
