        <WPFAutoCompleteTextbox:AutoCompleteTextBox x:Name="txt_hesab_code" HorizontalAlignment="Left" Height="35" Margin="426,223,0,0" VerticalAlignment="Top" Width="140" PreviewTextInput="txt_hesab_code_PreviewTextInput"/>
        <Label Content="کد تفصیل " Style="{StaticResource LabelStyle}" HorizontalAlignment="Left" Margin="326,223,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.037,-0.167" Width="95"/>
        <WPFAutoCompleteTextbox:AutoCompleteTextBox x:Name="txt_hesab_eng_name" HorizontalAlignment="Left" Height="35" Margin="181,223,0,0" VerticalAlignment="Top" Width="140" PreviewTextInput="txt_hesab_eng_name_PreviewTextInput"/>
